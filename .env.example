# Database API Configuration
DB_API_KEY=your_api_key_here
DB_API_URL=your_server_url_here

# Cloudflare R2 Configuration
# Note: R2 bucket is bound directly in Cloudflare Pages settings
# You only need to configure the public URL for accessing uploaded files
R2_PUBLIC_URL=https://your-custom-domain.com

# Session Secret for securing sessions
SESSION_SECRET=your_session_secret_here

# Disable Setup Page after initial setup
SETUP_DISABLED=false
